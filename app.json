{
  "name": "hailakka",
  "displayName": "hailakka",
  "expo": {
    "name": "hailakka",
    "slug": "hailakka",
    "version": "0.2.0",
    "platforms": ["ios", "android", "web"],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "highlakka",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfaceStyle": "automatic",
